	void	OnButton(long ID)
	{	
		switch(ID)
		{					
			case BID_CSMSRCH:
				break;
			case BID_OBJSRCH:
				break;
			case BID_BARSRCH:
				break;
			case BID_SEARCH:
				break;			
			case BID_HOME:
				Card_Move("MENU");
				break;
			case BID_MENU:
				Card_Move("SC_MENU");			
				break;
			case BID_KEYBOARD:
				break;
			case BID_SCREEN:
				break;
			case BID_CMMNWORK:
				break;
			case BID_PREV:
				Card_Move("SC_LST");
				break;	
			case BID_EXIT:
				g_Exit();
				break;
		}
	}		
