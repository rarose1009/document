//┌─────────────────────────────────────┐
//│ 				   『	OnInit Function  』  				              │
//└─────────────────────────────────────┘
	void	OnInit(char bFirst)
	{
		switch (bFirst)
		{
			case INIT_MAIN:
				switch (theDevInfo.m_nType)
				{
					case FAMILY_PDA:
						CREATE_GLOBAL_BUTTON (SysButRes_MAIN);
						CREATE_DIALOG_OBJECT (DlgRes_P, SIZEOF(DlgRes_P));
						
						ListCtrl_AddItem (Get_hDlgCtrlByID(CMB_GOO+2), "구(구주소)", 0, ICON_NONE);
						ListCtrl_AddItem (Get_hDlgCtrlByID(CMB_DONG+2), "동", 0, ICON_NONE);
						ListCtrl_AddItem (Get_hDlgCtrlByID(CMB_NWGOO+2), "구(신주소)", 0, ICON_NONE);
						ListCtrl_AddItem (Get_hDlgCtrlByID(CMB_STREET+2), "도로명", 0, ICON_NONE);
						SetStyle();
						break;
					default:
						CREATE_DIALOG_OBJECT (DlgRes, SIZEOF(DlgRes));
						EditCtrl_SetAlign( Get_hDlgCtrlByID(TXT_TITLE), EDITALIGN_MIDDLE);
						
						ListCtrl_AddItem (Get_hDlgCtrlByID(CMB_GOO+2), "구(구주소)", 0, ICON_NONE);
						ListCtrl_AddItem (Get_hDlgCtrlByID(CMB_DONG+2), "동", 0, ICON_NONE);
						ListCtrl_AddItem (Get_hDlgCtrlByID(CMB_NWGOO+2), "구(신주소)", 0, ICON_NONE);
						ListCtrl_AddItem (Get_hDlgCtrlByID(CMB_STREET+2), "도로명", 0, ICON_NONE);
						SetStyle();
						break;		
				}
				break;
		}
	}

