//┌─────────────────────────────────────┐
//│ 				   『	OnInit Function  』  				              │
//└─────────────────────────────────────┘
	void	OnInit(char bFirst)
	{
		switch (bFirst)
		{
			case INIT_MAIN:
				switch (theDevInfo.m_nType)
				{
					case FAMILY_PDA:
						CREATE_DIALOG_OBJECT (DlgRes_P, SIZEOF(DlgRes_P));
						
						ListCtrl_AddItem (Get_hDlgCtrlByID(CMB_AD+2), "전단지", 0, ICON_NONE);
						ListCtrl_AddItem (Get_hDlgCtrlByID(CMB_JUNWI+2), "유", 0, ICON_NONE);
						SetStyle();
						break;
					default:
						CREATE_DIALOG_OBJECT (DlgRes, SIZEOF(DlgRes));
						EditCtrl_SetAlign( Get_hDlgCtrlByID(TXT_TITLE), EDITALIGN_MIDDLE);

						ListCtrl_AddItem (Get_hDlgCtrlByID(CMB_AD+2), "전단지", 0, ICON_NONE);
						ListCtrl_AddItem (Get_hDlgCtrlByID(CMB_JUNWI+2), "유", 0, ICON_NONE);
						SetStyle();
						break;		
				}
				break;
		}
	}
	
